{
  "name": "ya-language-review",
  "displayName": "yet another Re:VIEW plugin",
  "description": "",
  "version": "0.0.1",
  "publisher": "erukiti",
  "engines": {
    "vscode": "^1.19.0"
  },
  "categories": [
    "Languages"
  ],
  "main": "./out/extension",
  "activationEvents": [
    "onLanguage:review",
    "onCommand:review.showPreview"
  ],
  "contributes": {
    "commands": [
      {
        "command": "review.showPreview",
        "title": "Show Re:VIEW preview",
        "when": "editorTextFocus"
      }
    ],
    "languages": [
      {
        "id": "review",
        "aliases": [
          "Re:VIEW",
          "review"
        ],
        "extensions": [
          ".re"
        ],
        "configuration": "./review.configuration.json"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "postinstall": "node ./node_modules/vscode/bin/install",
    "test": "npm run compile && node ./node_modules/vscode/bin/test"
  },
  "devDependencies": {
    "@types/mocha": "^2.2.42",
    "@types/node": "^9.3.0",
    "prettier": "^1.10.2",
    "tslint": "^5.9.1",
    "tslint-config": "0.0.1",
    "tslint-config-prettier": "^1.6.0",
    "tslint-plugin-prettier": "^1.3.0",
    "typescript": "^2.6.1",
    "vscode": "^1.1.6"
  }
}
